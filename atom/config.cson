"*":
  autosave:
    enabled: true
  core:
    audioBeep: false
    disabledPackages: [
      "archive-view"
      "autocomplete-atom-api"
      "autocomplete-plus"
      "autocomplete-snippets"
      "autoflow"
      "background-tips"
      "bookmarks"
      "command-palette"
      "dev-live-reload"
      "deprecation-cop"
      "image-view"
      "language-todo"
      "link"
      "wrap-guide"
      "welcome"
      "styleguide"
      "symbols-view"
      "snippets"
      "encoding-selector"
      "metrics"
      "exception-reporting"
      "spell-check"
    ]
    openEmptyEditorOnStart: false
  "custom-title":
    template: "<% if (projectName) { %><%= projectName %><% } %>"
  editor:
    fontFamily: "Consolas"
    invisibles: {}
    showInvisibles: true
    tabLength: 4
    tabType: "soft"
  "find-and-replace": {}
  "markdown-preview":
    useGitHubStyle: true
  "one-dark-ui":
    layoutMode: "Spacious"
  "spell-check": {}
  tabs:
    usePreviewTabs: true
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
    sortFoldersBeforeFiles: false
  welcome:
    showOnStartup: false
